{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primereact": "^6.4.1",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-router": "^5.2.0",
    "react-scripts": "3.2.0",
    "react-transition-group": "^4.4.2",
    "web3": "1.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "xo": {
    "extends": "xo-react",
    "webpack": true,
    "globals": [
      "test",
      "expect",
      "document",
      "window",
      "window.ethereum",
      "ethereum"
    ],
    "rules": {
      "react/prop-types": "off",
      "react/jsx-key": "off",
      "import/no-unassigned-import": "off",
      "unicorn/filename-case": [
        "error",
        {
          "case": "pascalCase"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-xo-react": "latest",
    "xo": "latest"
  }
}
